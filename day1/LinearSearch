package hundreddays;

//linear search

public class day1a_100day {

	public static void main(String[] args) {
		int[] a= {5,2,43,7,21,6,90};
		int element=43;
		int pos=-1;
		
		for(int i=0;i<a.length;i++) 
		{
			if(a[i]==element) 
			{
				pos=i;
				break;
			}
		}
		System.out.println(pos!=-1 ? +element+" found at index "+ pos : " element not found");
	}

}
